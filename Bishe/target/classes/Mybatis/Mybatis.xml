<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration  
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"  
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <typeAliases>
        <typeAlias type="com.qzq.bean.UserBasicInformation" alias="userBasic"/>
        <typeAlias type="com.qzq.bean.UserInformation" alias="userInfo"/>
        <typeAlias type="com.qzq.bean.FileInformation" alias="fileInfo"/>
        <typeAlias type="com.qzq.bean.FileComment" alias="fileComment"/>
        <typeAlias type="com.qzq.bean.Invitation" alias="invitation"/>
    </typeAliases>  
    <mappers>
        <mapper resource="Mybatis/Mapping/user_basic_mapping.xml" />
        <mapper resource="Mybatis/Mapping/user_mapping.xml" />
        <mapper resource="Mybatis/Mapping/file_mapping.xml" />
        <mapper resource="Mybatis/Mapping/file_comment_mapping.xml" />
        <mapper resource="Mybatis/Mapping/invitation_mapping.xml" />
    </mappers>
</configuration>